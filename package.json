{
  "name": "cypress-codemod",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "cypress-codemod": "dist/codemod.js"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/danadajian/cypress-codemod.git"
  },
  "bugs": {
    "url": "https://github.com/danadajian/cypress-codemod/issues"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@babel/core": "7.20.7",
    "chalk": "4.1.2",
    "commander": "9.4.1",
    "prettier": "2.8.0"
  },
  "devDependencies": {
    "@types/jest": "28.1.7",
    "@types/lodash": "4.14.191",
    "cypress": "12.2.0",
    "eslint": "8.26.0",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-github": "4.4.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "27.1.3",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-typescript": "0.14.0",
    "husky": "8.0.2",
    "jest": "28.1.3",
    "semantic-release": "19.0.5",
    "ts-jest": "28.0.8",
    "ts-node": "10.9.1",
    "typescript": "4.9.4"
  },
  "scripts": {
    "build": "tsc --skipLibCheck && mkdir -p node_modules/cypress-codemod/dist && cp -r dist package.json node_modules/cypress-codemod",
    "cli": "ts-node src/codemod.ts",
    "format": "prettier --write .",
    "format-check": "prettier .",
    "lint": "eslint --quiet --fix src/**/*",
    "prepare": "husky install",
    "test": "jest"
  },
  "jest": {
    "transform": {
      "^.+\\.(j|t)sx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "clearMocks": true
  }
}
